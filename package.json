{
  "name": "simple-mern-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "npm run build",
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-api": "cross-env NODE_ENV=development nodemon --exec babel-node ./bin/api.js",
    "dev-server": "cross-env NODE_ENV=development nodemon --exec babel-node ./bin/server.js",
    "webpack-dev-server": "cross-env NODE_ENV=development babel-node ./bin/webpack-dev-server.js",
    "start": "concurrently --kill-others \"npm run start-api\" \"npm run start-server\"",
    "start-api": "cross-env NODE_ENV=production babel-node ./bin/api.js",
    "start-server": "cross-env NODE_ENV=production babel-node ./bin/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odarino/simple-mern-generator.git"
  },
  "author": "Odarino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/odarino/simple-mern-generator/issues"
  },
  "homepage": "https://github.com/odarino/simple-mern-generator#readme",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "nodemon": "^1.10.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.25.0",
    "electron-to-chromium": "^1.3.21",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-proxy": "^1.15.1",
    "postcss-loader": "^0.13.0",
    "pretty-error": "^2.0.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.6",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.5.8"
  }
}
